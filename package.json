{
  "name": "@haixing_hu/case-format",
  "version": "1.0.0",
  "description": "A simple JavaScript library for converting the case of ASCII strings.",
  "author": "Haixing Hu",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/js-case-format.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/js-case-format/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/js-case-format",
  "keywords": [
    "javascript",
    "string",
    "case",
    "conversion"
  ],
  "main": "dist/case-format.cjs.min.js",
  "module": "dist/case-format.esm.min.mjs",
  "exports": {
    ".": {
      "require": "./dist/case-format.cjs.min.js",
      "import": "./dist/case-format.esm.min.mjs"
    }
  },
  "scripts": {
    "clean": "rm -rvf dist doc",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs",
    "build:all": "yarn clean && yarn build:dev && yarn build && yarn doc",
    "test": "jest",
    "lint": "eslint ./src ./test",
    "doc": "jsdoc -c jsdoc.json",
    "es5": "check-es-version -e 5 -s true",
    "deploy": "yarn & yarn lint && yarn test && yarn build:all && npm publish --registry='https://registry.npmjs.com/' --access public"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "peerDependencies": {
    "@babel/runtime": "^7.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/runtime": "^7.23.2",
    "@haixing_hu/eslint-config": "^1.3.1",
    "@haixing_hu/jsdoc-minami": "^1.5.0",
    "@haixing_hu/rollup-builder": "^1.0.2",
    "@jest/core": "^29.7.0",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "check-es-version": "^1.4.2",
    "core-js": "^3.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-extended": "^4.0.1",
    "jsdoc": "^4.0.2",
    "rollup": "^4.1.4",
    "rollup-plugin-analyzer": "^4.0.0"
  },
  "packageManager": "yarn@4.0.2"
}
